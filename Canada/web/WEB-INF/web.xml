<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registrate</servlet-name>
        <servlet-class>Registrate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactReview</servlet-name>
        <servlet-class>ContactReview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listofusers</servlet-name>
        <servlet-class>Listofusers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginS</servlet-name>
        <servlet-class>LoginS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listofreviews</servlet-name>
        <servlet-class>Listofreviews</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registrate</servlet-name>
        <url-pattern>/Registrate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactReview</servlet-name>
        <url-pattern>/ContactReview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listofusers</servlet-name>
        <url-pattern>/Listofusers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginS</servlet-name>
        <url-pattern>/LoginS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listofreviews</servlet-name>
        <url-pattern>/Listofreviews</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
